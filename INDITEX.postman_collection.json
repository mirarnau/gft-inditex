					"script": {
						"exec": [
							"pm.test(\"Validates response status code\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Validates response body properties\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"\r",
							"    pm.expect(jsonData).to.have.property('productId').that.equals(35455);\r",
							"    pm.expect(jsonData).to.have.property('brandId').that.equals(1);\r",
							"    pm.expect(jsonData).to.have.property('priceList').that.equals(1);\r",
							"    pm.expect(jsonData).to.have.property('startDate').that.equals(\"2020-06-14T00:00:00\");\r",
							"    pm.expect(jsonData).to.have.property('endDate').that.equals(\"2020-12-31T23:59:59\");\r",
							"    pm.expect(jsonData).to.have.property('price').that.equals(35.5);\r",
							"});"
						],
